lay: flex;\n  overflow-x: auto;\n  gap: 15px;\n  scroll-behavior: smooth;\n  padding: 10px 0;\n  scrollbar-width: none; /* Firefox 스크롤바 숨기기 */\n}\n\n.slider-content::-webkit-scrollbar {\n  display: none; /* Chrome, Safari, Edge 스크롤바 숨기기 */\n}\n</style>\n\n\n"],"mappings":";OAKAA,UAAuB;OAgCfC,UAA+B;;EAnC9BC,KAAK,EAAC;AAAa;;EASlBA,KAAK,EAAC;AAAW;;EAwBlBA,KAAK,EAAC;AAAc;;;uBAlC3BC,mBAAA,CA0CM;IA1CAD,KAAK,EADbE,eAAA;MAAA,mBAC+CC,KAAA,CAAAC;IAAU;MACrDC,mBAAA,CA+BM,OA/BNC,UA+BM,GA9BJC,YAAA,CAMcC,sBAAA;IANDC,EAAE,EAAC;EAAO;IAH7BC,OAAA,EAAAC,QAAA,CAIQ,MAIPC,MAAA,QAAAA,MAAA,OAJOP,mBAAA,CAIP;MAHDQ,GAAuB,EAAvBf,UAAuB;MACvBgB,GAAG,EAAC,MAAM;MACVd,KAAK,EAAC;;IAPNe,CAAA;MAWMV,mBAAA,CAqBK,MArBLW,UAqBK,GApBHX,mBAAA,CAIH,aAHLE,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC;EAAO;IAbvBC,OAAA,EAAAC,QAAA,CAawB,MAExBC,MAAA,QAAAA,MAAA,OAfAK,gBAAA,CAawB,KAExB,E;IAfAF,CAAA;QAiBQV,mBAAA,CAIH,aAHLE,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC;EAAU;IAlB1BC,OAAA,EAAAC,QAAA,CAkB2B,MAE3BC,MAAA,QAAAA,MAAA,OApBAK,gBAAA,CAkB2B,UAE3B,E;IApBAF,CAAA;QAsBQV,mBAAA,CAIH,aAHLE,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC;EAAS;IAvBzBC,OAAA,EAAAC,QAAA,CAuB0B,MAE1BC,MAAA,QAAAA,MAAA,OAzBAK,gBAAA,CAuB0B,QAE1B,E;IAzBAF,CAAA;QA2BQV,mBAAA,CAIH,aAHLE,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC;EAAW;IA5B3BC,OAAA,EAAAC,QAAA,CA4B4B,MAE5BC,MAAA,QAAAA,MAAA,OA9BAK,gBAAA,CA4B4B,aAE5B,E;IA9BAF,CAAA;YAmCIV,mBAAA,CAOM,OAPNa,UAOM,GANJb,mBAAA,CAKC;IAJCQ,GAA+B,EAA/Bd,UAA+B;IAC/Be,GAAG,EAAC,SAAS;IACbd,KAAK,EAAC,cAAc;IACnBmB,OAAK,EAAAP,MAAA,QAAAA,MAAA,UAAAQ,IAAA,KAAEC,QAAA,CAAAC,MAAA,IAAAD,QAAA,CAAAC,MAAA,IAAAF,IAAA,CAAM","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}