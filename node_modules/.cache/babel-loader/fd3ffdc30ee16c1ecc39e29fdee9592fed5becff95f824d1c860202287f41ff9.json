{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport Navbar from \"@/components/Navbar.vue\";\nexport default {\n  name: \"WishList\",\n  components: {\n    Navbar\n  },\n  setup() {\n    const store = useStore();\n    const wishlist = computed(() => store.getters.wishlist);\n    const toggleWishlist = movie => {\n      store.dispatch(\"toggleWishlist\", movie);\n    };\n    return {\n      wishlist,\n      toggleWishlist\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","Navbar","name","components","setup","store","wishlist","getters","toggleWishlist","movie","dispatch"],"sources":["C:\\Users\\hyemi\\WSD-Assignment-04\\src\\views\\WishList.vue"],"sourcesContent":["<template>\r\n  <div class=\"wishlist\">\r\n    <Navbar />\r\n    <h2>내가 찜한 콘텐츠</h2>\r\n\r\n    <div\nv-if=\"wishlist.length === 0\"\nclass=\"empty-wishlist\"\n>\r\n      <p>찜한 영화가 없습니다.</p>\r\n    </div>\r\n    <div\nv-else\nclass=\"wishlist-movies\"\n>\r\n      <div\r\n        v-for=\"movie in wishlist\"\r\n        :key=\"movie.id\"\r\n        class=\"wishlist-movie\"\r\n        :style=\"{ backgroundImage: `url(https://image.tmdb.org/t/p/w500/${movie.poster_path})` }\"\r\n      >\r\n        <div class=\"movie-info\">\r\n          <h4>{{ movie.title }}</h4>\r\n          <button\nclass=\"remove-btn\"\n@click=\"toggleWishlist(movie)\"\n>\n삭제\n</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from \"vue\";\r\nimport { useStore } from \"vuex\";\r\nimport Navbar from \"@/components/Navbar.vue\";\r\n\r\nexport default {\r\n  name: \"WishList\",\r\n  components: { Navbar },\r\n  setup() {\r\n    const store = useStore();\r\n    const wishlist = computed(() => store.getters.wishlist);\r\n\r\n    const toggleWishlist = (movie) => {\r\n      store.dispatch(\"toggleWishlist\", movie);\r\n    };\r\n\r\n    return { wishlist, toggleWishlist };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.wishlist {\r\n  padding: 20px;\r\n  color: white;\r\n}\r\n\r\n.wishlist h2 {\r\n  margin-bottom: 20px;\r\n  font-size: 1.8em;\r\n}\r\n\r\n.wishlist-movies {\r\n  display: flex;\r\n  gap: 15px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.wishlist-movie {\r\n  width: 150px;\r\n  height: 225px;\r\n  background-size: cover;\r\n  background-position: center;\r\n  border-radius: 8px;\r\n  position: relative;\r\n}\r\n\r\n.movie-info {\r\n  background: rgba(0, 0, 0, 0.6);\r\n  padding: 10px;\r\n  text-align: center;\r\n  border-radius: 0 0 8px 8px;\r\n}\r\n\r\n.remove-btn {\r\n  margin-top: 10px;\r\n  padding: 5px 10px;\r\n  border: none;\r\n  background-color: #e50914;\r\n  color: white;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-btn:hover {\r\n  background-color: #b00610;\r\n}\r\n\r\n.navbar {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  z-index: 1000;\r\n  background-color: #121212; /* 배경색 추가 */\r\n  border-bottom: 1px solid #333; /* 하단 구분선 추가 */\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEF;EAAO,CAAC;EACtBG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC,CAAC;IACxB,MAAMM,QAAO,GAAIP,QAAQ,CAAC,MAAMM,KAAK,CAACE,OAAO,CAACD,QAAQ,CAAC;IAEvD,MAAME,cAAa,GAAKC,KAAK,IAAK;MAChCJ,KAAK,CAACK,QAAQ,CAAC,gBAAgB,EAAED,KAAK,CAAC;IACzC,CAAC;IAED,OAAO;MAAEH,QAAQ;MAAEE;IAAe,CAAC;EACrC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}